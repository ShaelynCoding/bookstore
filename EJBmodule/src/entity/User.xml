<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entity.User">
        <table name="user" schema="Store" catalog=""/>
        <attributes>
            <id name="userId">
                <column name="userId"/>
            </id>
            <basic name="userName">
                <column name="userName"/>
            </basic>
            <basic name="userPwd">
                <column name="userPwd"/>
            </basic>
            <basic name="userEmail">
                <column name="userEmail"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
