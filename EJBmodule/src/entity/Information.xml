<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="entity.Information">
        <table name="Information" schema="Store" catalog=""/>
        <attributes>
            <id name="infoId">
                <column name="infoId"/>
            </id>
            <basic name="userId">
                <column name="userId"/>
            </basic>
            <basic name="bookIsdn">
                <column name="bookISDN"/>
            </basic>
            
            <basic name="buyNum">
                <column name="buyNum"/>
            </basic>
            <basic name="time">
                <column name="time"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
